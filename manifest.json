{
  "name": "G-Star RAW quantity affixer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "It adds quantity after the size name",
  "homepage_url": "https://google.com",
  "icons": {
    "16": "icons/raw32.png",
    "48": "icons/raw200.png",
    "128": "icons/raw200.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.g-star.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.g-star.com/*"
      ],
      "js": [
        "js/inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/main.js"
  ]
}